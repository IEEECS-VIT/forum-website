<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <style media="screen">
            td{
                border: 1px black solid;
            }
        </style>
    </head>
    <body>
        <table>
            <thead>
                <tr>

                    <td>
                        Thread Name
                    </td>
                    <td>
                        Owner
                    </td>
                    <td>
                        Replies
                    </td>
                    <td>
                        Active
                    </td>
                </tr>
            </thead>
            <tbody>
                <%  for(var i=0;  i<threads.length; i++){ %>
                    <tr>
                        <td>
                            <%=  threads[i].name %>
                        </td>
                        <td>
                            <%=  threads[i].creator  %>
                        </td>
                        <td>
                            <%=  threads[i].replies %>
                        </td>
                        <td>
                            <%=  threads[i].isActive ?   "Yes"   :   "No"    %>
                        </td>
                    </tr>
                <%  }   %>
            </tbody>
        </table>
    </body>
</html>
